<h1>New soumission_europe_partenaire</h1>

<%= error_messages_for :soumission_europe_partenaire %>

<% form_for(:soumission_europe_partenaire, :url => soumission_europe_partenaires_path) do |f| %>
  <p>
    <b>Soumissions</b><br />
    <%= f.text_field :soumission_id %>
  </p>

  <p>
    <b>Nom</b><br />
    <%= f.text_field :nom %>
  </p>

  <p>
    <b>Etablissement gestionnaire</b><br />
    <%= f.text_field :etablissement_gestionnaire %>
  </p>

  <p>
    <b>Ville</b><br />
    <%= f.text_field :ville %>
  </p>

  <p>
    <b>Pays</b><br />
    <%= f.text_field :pays %>
  </p>

  <p>
    <b>Type</b><br />
    <%= f.text_field :type %>
  </p>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', soumission_europe_partenaires_path %>
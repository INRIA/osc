<h1>Editing notification_partenaire</h1>

<%= error_messages_for :notification_partenaire %>

<% form_for(:notification_partenaire, :url => notification_partenaire_path(@notification_partenaire), :html => { :method => :put }) do |f| %>
  <p>
    <b>Notification</b><br />
    <%= f.text_field :notification_id %>
  </p>

  <p>
    <b>Nom</b><br />
    <%= f.text_field :nom %>
  </p>

  <p>
    <b>Etablissement gestionnaire</b><br />
    <%= f.text_field :etablissement_gestionnaire %>
  </p>

  <p>
    <b>Ville</b><br />
    <%= f.text_field :ville %>
  </p>

  <p>
    <b>Pays</b><br />
    <%= f.text_field :pays %>
  </p>

  <p>
    <b>Localisation</b><br />
    <%= f.text_field :localisation %>
  </p>

  <p>
    <%= submit_tag "Update" %>
  </p>
<% end %>

<%= link_to 'Show', notification_partenaire_path(@notification_partenaire) %> |
<%= link_to 'Back', notification_partenaires_path %>
<h1>Editing soumission_personnel</h1>

<%= error_messages_for :soumission_personnel %>

<% form_for(:soumission_personnel, :url => soumission_personnel_path(@soumission_personnel), :html => { :method => :put }) do |f| %>
  <p>
    <b>Soumissions</b><br />
    <%= f.text_field :soumissions_id %>
  </p>

  <p>
    <b>Nom</b><br />
    <%= f.text_field :nom %>
  </p>

  <p>
    <b>Prenom</b><br />
    <%= f.text_field :prenom %>
  </p>

  <p>
    <b>Statut</b><br />
    <%= f.text_field :statut %>
  </p>

  <p>
    <b>Tutelle</b><br />
    <%= f.text_field :tutelle %>
  </p>

  <p>
    <b>Pourcentage</b><br />
    <%= f.text_field :pourcentage %>
  </p>

  <p>
    <%= submit_tag "Update" %>
  </p>
<% end %>

<%= link_to 'Show', soumission_personnel_path(@soumission_personnel) %> |
<%= link_to 'Back', soumission_personnels_path %>
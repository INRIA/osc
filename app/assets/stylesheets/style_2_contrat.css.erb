	color:#D5C49E;
	color:#B5A686;
}

#entete h1 strong{
	color:#FFF;
}


/****************************************
		#onglet_tab
*****************************************/

#onglets_tab{
	background: transparent url(<%= asset_path 'tab_control_background.png' %>) repeat-x left center;
	border:none;
	border-bottom:1px solid #fff;
}

#onglets_tab a{
	color:#E9DFB4;
	background: transparent url(<%= asset_path 'tab_control_sep_barre.png' %>) no-repeat right center;
}
#onglets_tab a.active{
	background: transparent url(<%= asset_path 'tab_control_background_a_first_active.png' %>) no-repeat right center;
}
#onglets_tab  a.before_active{
	background: transparent url(<%= asset_path 'tab_control_sep_barre_before_active.png' %>) no-repeat right center;
}

/*	--------    .pagination    ------- */

.pagination a {
  border: 1px solid #ccc;
  color: #205480;
}

.pagination a:hover, .pagination a:active {
  color: #205480;
}
.pagination span.current {
  background-color: #205480;
  color: #FFF;
}
.pagination span.disabled {
  border: 1px solid #eee;
  color: #ddd;
}


/* Sub Menu */
#menu a.current-with-sub-menu{
	background-color:#695D4D;	
}
#sub-menu{
	padding-left:10px;
	background-color:#A5937C;
	border-top:2px solid #695D4D;
	border-bottom:2px solid #695D4D;
	font-size:11px;
}
#sub-menu .wrapper{
	margin:0pt auto;
	width:950px;
}

#sub-menu a{
	display:block;
	float:left;
	padding:0em 20px;
	background-color: transparent;
	margin-right:5px;
	color:#fff;
	text-decoration: none;
	border-top:3px solid #A5937C;
	border-bottom:3px solid #A5937C;
	}
	
#sub-menu a.current{
	color:#fff;
	font-weight:bold;
	border-bottom:3px solid #695D4D;
}
#sub-menu a:hover{
	border-bottom:3px solid #695D4D;
}


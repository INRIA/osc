%>


body{
	border-top:4px solid #D8BE99;
}
a {
	color:#205480;
}
a:hover{
	color:#000;
}
.error {
	color:#992629;
}

form{
	background-color:transparent;
	border:none;
	padding:2px 0px 0px 0px;
}
fieldset{
	margin-bottom:2em;
}

fieldset legend{
	font-size:1.5em;
}
p.init{
	border: 1px solid #aaa;
	padding:10px;
	color:#888;
	text-align:center;
}
#entete, #menu{
	background-color:#221F18;
}
#entete{
	border-top: 1px solid #000;	
}

